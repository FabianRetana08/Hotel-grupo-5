        ID_TIPO_RESERVA,
        NOMBRE,
        DESCRIPCION,
        ID_ESTADO
    ) VALUES (
        p_ID_TIPO_RESERVA,
        p_NOMBRE,
        p_DESCRIPCION,
        p_ID_ESTADO
    );
    COMMIT;
END FIDE_TIPO_RESERVA_TB_INSERTAR_SP;

