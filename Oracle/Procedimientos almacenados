--FIDE_PRODUCTO_TB
--Insertar FIDE_PRODUCTO_TB
CREATE OR REPLACE PROCEDURE FIDE_PRODUCTO_TB_INSERTAR_SP (
    p_ID_PRODUCTO IN NUMBER,
    p_NOMBRE IN VARCHAR2,
    p_DESCRIPCION IN VARCHAR2,
    p_ID_CATEGORIA IN NUMBER,
    p_ID_SUCURSAL IN NUMBER,
    p_ID_ESTADO IN NUMBER
) AS
BEGIN
    INSERT INTO FIDE_PRODUCTO_TB (
        ID_PRODUCTO,
        NOMBRE,
        DESCRIPCION,
        ID_CATEGORIA,
        ID_SUCURSAL,
        ID_ESTADO
    ) VALUES (
        p_ID_PRODUCTO,
        p_NOMBRE,
        p_DESCRIPCION,
        p_ID_CATEGORIA,
        p_ID_SUCURSAL,
        p_ID_ESTADO
    );
    COMMIT;
END FIDE_PRODUCTO_TB_INSERTAR_SP;

/
--Obtener FIDE_PRODUCTO_TB
CREATE OR REPLACE PROCEDURE FIDE_PRODUCTO_TB_OBTENER_SP (
    p_resultado OUT SYS_REFCURSOR
) AS
BEGIN
    OPEN p_resultado FOR
    SELECT ID_PRODUCTO, NOMBRE, DESCRIPCION, ID_CATEGORIA, ID_SUCURSAL, ID_ESTADO
    FROM FIDE_PRODUCTO_TB;
END FIDE_PRODUCTO_TB_OBTENER_SP;


--Actualizar FIDE_PRODUCTO_TB
CREATE OR REPLACE PROCEDURE FIDE_PRODUCTO_TB_ACTUALIZAR_SP (
    p_ID_PRODUCTO IN NUMBER,
    p_NOMBRE IN VARCHAR2,
    p_DESCRIPCION IN VARCHAR2,
    p_ID_CATEGORIA IN NUMBER,
    p_ID_SUCURSAL IN NUMBER,
    p_ID_ESTADO IN NUMBER
) AS
BEGIN
    UPDATE FIDE_PRODUCTO_TB
    SET
        NOMBRE = p_NOMBRE,
        DESCRIPCION = p_DESCRIPCION,
        ID_CATEGORIA = p_ID_CATEGORIA,
        ID_SUCURSAL = p_ID_SUCURSAL,
        ID_ESTADO = p_ID_ESTADO
    WHERE
        ID_PRODUCTO = p_ID_PRODUCTO;
    COMMIT;
END FIDE_PRODUCTO_TB_ACTUALIZAR_SP;
/
--Insertar FIDE_TIPO_RESERVA
CREATE OR REPLACE PROCEDURE FIDE_TIPO_RESERVA_TB_INSERTAR_SP (
    p_ID_TIPO_RESERVA IN NUMBER,
    p_NOMBRE IN VARCHAR2,
    p_DESCRIPCION IN VARCHAR2,
    p_ID_ESTADO IN NUMBER
) AS
BEGIN
