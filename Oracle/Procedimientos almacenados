BEGIN
    UPDATE FIDE_CATEGORIA_PRODUCTO_TB
    SET NOMBRE = p_nombre,
        DESCRIPCION = p_descripcion,
        ID_ESTADO = p_id_estado
    WHERE ID_CATEGORIA = p_id_categoria;
END FIDE_CATEGORIA_PRODUCTO_TB_ACTUALIZAR_SP;
